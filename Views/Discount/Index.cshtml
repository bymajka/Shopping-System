@model IEnumerable<ShoppingSystem.Models.Customer>
@{
    ViewData["Title"] = "My discount";
}
@{

}
<h1>Discount</h1>
<table class="table">
	<thead>
		<tr>
			<th>
				Discount
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					Your discount
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Discount)
				</td>
			</tr>
		}
	</tbody>
</table>
<div>
    <a asp-action="Index" asp-controller="Home">Back to List</a>
</div>

